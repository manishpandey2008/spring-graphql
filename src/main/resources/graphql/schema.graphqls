type Query{
    allBooks:[Book]
    getBook(bookId:Int):Book
    getBookByCriteria(searchBuilder:[SearchCriteria]):[Book]
    createBook(book:BookInput):Book
    updateBook(book:BookInput):Book
}

type Book{
    id:ID!
    title:String
    desc:String
    author:String
    price:Float
    page:Int
    createdTimestamp:String
}

input BookInput{
    id: Int
    title:String
    desc:String
    author:String
    price:Float
    page:Int
    createdTimestamp:String
}


